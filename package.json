{
    "name": "atomblast.io",
    "version": "1.0.0",
    "description": "First attempt at multiplayer online io game",
    "main": "index.js",
    "scripts": {
        "test": "echo \"$(tput setaf 1)WARN$(tput sgr0): Unit testing not configured.\nSKIPPING REQUIREMENT\" && exit 0",
        "watch": "watchify src/client/js/app.js -o src/client/js/public/build.js -v",
        "server": "nodemon src/server/entry.js",
        "start": "npm-run-all --parallel watch server",
        "build": "browserify src/client/js/app.js -o  src/client/js/public/build.js",
        "lint": "echo 'Please wait. Lint Checking...\n' && ./node_modules/.bin/eslint src/",
        "precommit": "npm lint && npm test",
        "prepush": "npm lint && echo 'Prepush build skipped' && npm test"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/BananiumLabs/AtomBlast.io.git"
    },
    "keywords": [
        "io",
        "game"
    ],
    "author": "Bananium Labs",
    "license": "MIT",
    "contributors": [
        "Ben Cuan <ben@bananiumlabs.com> (https://github.com/dbqeo)",
        "Vikram Peddinti <vikram@bananiumlabs.com> (https://github.com/darkflamex1)",
        "Eric Qian <eric@enumc.com> (https://github.com/EnumC)"
    ],
    "bugs": {
        "url": "https://github.com/BananiumLabs/AtomBlast.io/issues"
    },
    "homepage": "https://github.com/BananiumLabs/AtomBlast.io#readme",
    "dependencies": {
        "babel-preset-env": "^1.7.0",
        "babel-register": "^6.26.0",
        "colors": "^1.2.4",
        "express": "^4.13.0",
        "pixi.js": "^4.8.1",
        "socket.io": "^1.3.5",
        "sweetalert": "^2.1.0"
    },
    "devDependencies": {
        "babel-core": "^6.26.3",
        "babel-preset-es2015": "^6.24.1",
        "babelify": "^8.0.0",
        "browserify": "^16.2.0",
        "eslint": "^5.1.0",
        "husky": "^0.14.3",
        "nodemon": "^1.17.3",
        "npm-run-all": "^4.1.2",
        "npm-run-parallel": "^0.6.0",
        "watchify": "^3.11.0"
    },
    "browserify": {
        "transform": [
            [
                "babelify",
                {
                    "presets": [
                        "es2015"
                    ]
                }
            ]
        ]
    }
}
